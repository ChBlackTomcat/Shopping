<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.first.myapplication">
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity_welcome">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="activity.RegisterActivity"></activity>
        <activity android:name="activity.LoginActivity"></activity>
        <activity android:name="activity.MainActivity"></activity>
        <activity android:name="activity.GoodsActivity"></activity>
        <activity android:name="activity.RevisionPasswordActivity"></activity>
        <activity android:name="activity.RevisionMessageActivity"></activity>
        <activity android:name="activity.UserMessageActivity"></activity>
        <activity android:name="activity.ImageBaseActivity"></activity>
        <provider
            android:authorities="com.userMessage"
            android:name="contextprovider.UserMessageProvider">
        </provider>
    </application>

</manifest>